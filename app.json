{
  "expo": {
    "name": "OneMarket Pro",
    "slug": "onemarket-pro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2563EB"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow OneMarket Pro to access your photos for ID verification and profile picture.",
          "cameraPermission": "Allow OneMarket Pro to access your camera for ID verification and profile picture."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#2563EB"
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.onemarket.pro"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2563EB"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.onemarket.pro",
      "useNextNotificationsApi": true,
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7409abce-92a9-419c-ad18-510d3cf53e91"
      }
    },
    "owner": "paras0369"
  }
}
